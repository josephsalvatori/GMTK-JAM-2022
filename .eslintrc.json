{
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "globals": {
        "angular": false,
        "module": false,
        "inject": false,
        "document": false
    },
    "env": {
        "browser": true,
        "amd": true,
        "es6": true
    },
    "rules": {
        "no-async-promise-executor": "off",
        "no-case-declarations": "off",
        "no-console": "off",
        "no-unused-vars": "off"
    },
    "settings": {}
}
